    "vue-jest": "^4.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "babelConfig": true
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue,ts}",
      "!**/mytest.*",
      "!src/main.ts",
      "!src/router.ts",
      "!**/node_modules/**",
      "!**/*.d.ts",
      "!**/state.ts"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "./src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!vuikit)"
    ]
  }
}
