    "vue-sidebar-menu": "^4.6.2",
    "vue-upload-component": "^2.8.20",
    "vuex": "^3.5.1",
    "vuikit": "^0.8.10",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
